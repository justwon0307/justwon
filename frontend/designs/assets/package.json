{
  "name": "@justwon/assets",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "generate": "svg2tsx generate && prettier --write src",
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:dev": "vitest --watch"
  },
  "devDependencies": {
    "@justkits/svg2tsx": "catalog:",
    "@justwon/tsconfig": "workspace:*",
    "@justwon/vitest-config": "workspace:*",
    "@testing-library/react": "catalog:",
    "@types/react": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:"
  },
  "lint-staged": {
    "*.{json,md,yml,yaml}": "prettier --check",
    "*.{ts,tsx,js,jsx}": [
      "eslint .",
      "prettier --check"
    ]
  }
}
